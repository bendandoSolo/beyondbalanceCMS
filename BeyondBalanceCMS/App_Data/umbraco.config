    <training id="1078" key="5cb3c95d-aa25-4662-bae5-f7d14f410031" parentID="1055" level="2" creatorID="0" sortOrder="0" createDate="2020-02-14T19:56:05" updateDate="2020-02-14T19:56:05" nodeName="Training" urlName="training" path="-1,1055,1078" isDoc="" nodeType="1077" creatorName="Benjamin Roberts" writerName="Benjamin Roberts" writerID="0" template="1076" nodeTypeAlias="training" isPublished="true">
